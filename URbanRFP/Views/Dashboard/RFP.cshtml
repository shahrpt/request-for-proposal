                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
@section scripts{
    <script src="~/Content/tablesorter/jquery.metadata.js"></script>
    <script src="~/Content/tablesorter/jquery.tablesorter.min.js"></script>
    <script src="~/Content/Scripts/AppScripts/dashboard.js"></script>
}